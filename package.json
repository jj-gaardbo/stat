{
  "name": "react-electron",
  "version": "0.0.1",
  "private": true,
  "author": "JJGaardbo",
  "description": "This is a ReactElectron template",
  "dependencies": {
    "clean-webpack-plugin": "^0.1.18",
    "compression-webpack-plugin": "^1.1.7",
    "copy-webpack-plugin": "^4.4.1",
    "cross-env": "5.0.1",
    "electron-installer-debian": "^0.8.1",
    "electron-is-dev": "0.1.2",
    "electron-log": "^2.2.6",
    "electron-packager": "^11.0.1",
    "electron-updater": "^2.4.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "modernizr-webpack-plugin": "^1.0.6",
    "node-notifier": "^5.1.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.2.2",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-manifest-plugin": "^1.3.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "concurrently": "3.4.0",
    "css-loader": "^0.28.10",
    "electron": "2.0.0-beta5",
    "electron-builder": "^19.8.0",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.7.2",
    "prettier": "^1.4.4",
    "react-scripts": "1.0.7",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.2",
    "url-loader": "^0.5.9",
    "wait-on": "2.0.2"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "build": {
    "appId": "com.example.react-electron",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "publish": {
      "provider": "github"
    }
  },
  "scripts": {
    "start": "webpack-dev-server",
    "build": "webpack --config webpack.config.prod.js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:8080 && electron .\"",
    "electron-pack": "USE_SYSTEM_XORRISO=true build --em.main=build/electron.js",
    "package-linux": "electron-packager .  electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds",
    "electron-release": "USE_SYSTEM_XORRISO=true build --em.main=build/electron.js --publish=always",
    "preelectron-pack": "yarn build",
    "preelectron-release": "yarn build"
  }
}
